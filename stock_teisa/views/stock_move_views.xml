<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_move_line_detailed_operation_tree_inherit_teisa" model="ir.ui.view">
        <field name="name">stock.move.line.operations.tree.inherit.teisa</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='qty_done']" position="after">
                <field name="picking_code" invisible="1"/>
                <field name="price_unit" attrs="{'invisible': [('picking_code', '!=', 'outgoing')]}"/>
                <field name="subtotal" attrs="{'invisible': [('picking_code', '!=', 'outgoing')]}"/>
            </xpath>
        </field>
    </record>
</odoo>
